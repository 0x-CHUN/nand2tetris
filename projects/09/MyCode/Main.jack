/** Hello World program. */
class Main {
   function void main() {
      /* Prints some text using the standard library. */
	  var int i;
	  var int r;
	  let i = 5;
	  let r = Keyboard.readInt("Numbers: ");
	  while( i < r){
		do Screen.drawCircle(r, r, i);
		let i = i + 5;
	  }
      return;
   }
}
